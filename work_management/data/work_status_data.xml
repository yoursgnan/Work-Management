<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="status_todo" model="work.status">
            <field name="sequence">1</field>
            <field name="name">To Do</field>
            <field name="active">True</field>
        </record>

        <record id="status_doing" model="work.status">
             <field name="sequence">2</field>
            <field name="name">Doing</field>
            <field name="active">True</field>
        </record>

        <record id="status_onhold" model="work.status">
             <field name="sequence">3</field>
            <field name="name">On Hold</field>
            <field name="active">True</field>
        </record>

        <record id="status_completed" model="work.status">
            <field name="sequence">4</field>
            <field name="name">Completed</field>
            <field name="active">True</field>
            <field name="final_stage_bool">True</field>
        </record>
    </data>
</odoo>